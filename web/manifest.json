{
  "manifest_version": 3,
  "name": "Lazy 2FA",
  "version": "1.0",
  "description": "A Chrome extension to simplify two-factor authentication.",
  "permissions": [
    "storage"
  ],
  "host_permissions": [],
  "action": {
    "default_popup": "index.html"
  },
  "content_security_policy": {
     "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';"
  }
}
